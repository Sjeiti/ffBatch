{
  "name": "FFBatch",
  "version": "3.0.0-beta",
  "description": "An FFXML interpreter to spew out XML files needed for the FilterForge batch renderer. See http://ffbatch.ronvalstar.nl",
  "url": "https://github.com/Sjeiti/ffbatch.git",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "adm-zip-cli": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-minify": "^0.4.3",
    "body-parser": "^1.18.2",
    "chokidar-cli": "^2.0.0",
    "commander": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "del-cli": "^3.0.1",
    "express": "^4.16.3",
    "less-loader": "^6.2.0",
    "less-plugin-glob": "^3.0.0",
    "next-version": "^0.11.7",
    "npm-run-all": "^4.1.5",
    "open": "^6.4.0",
    "raw-loader": "^4.0.1",
    "serve-static": "^1.13.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-watch-files-plugin": "^1.0.3"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-alpha1"
  },
  "scripts": {
    "start": "run-p build serve",
    "serve": "node task/serve dist",
    "webpack": "npx webpack --config webpack.config.js",
    "clean": "del-cli dist -f",
    "build": "run-s clean \"webpack -- --env.development --watch\"",
    "build:prod": "run-s clean build:unzip build:json build:search \"webpack -- --env.production\" build:sitemap build:rss",
    "_serve:dist": "node task/serve --root=dist --port=8283",
    "_less": "node task/less --source=screen.less --target=screen.css --cwd=src/style/",
    "_less:watch": "chokidar \"src/style/**/*.less\" -c \"npm run less\"",
    "_version": "next-version package.json bower.json src/attractors.appcache src/js/attractors.js",
    "_version:beta": "next-version package.json bower.json src/attractors.appcache src/js/attractors.js --release=beta --patch",
    "_icons": "node task/icons",
    "_icomoon": "node task/icomoon"
  },
  "keywords": [
    "ffbatch"
  ],
  "author": "Ron Valstar (http://ronvalstar.nl/)"
}
